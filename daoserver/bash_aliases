alias psql-db='PGPASSWORD=$DB_PASSWORD psql -h $DB_PORT_5432_TCP_ADDR -p $DB_PORT_5432_TCP_PORT -d $DB_NAME -U docker'
alias eit='exit'
alias feedback='psql-db -c "select * from feedback;"'
alias accounts='psql-db -c "select * from account;"'
alias registrations='psql-db -c "select * from registration;"'
alias score-keys='psql-db -c "select * from score_key;"'
alias tournaments='psql-db -c "select * from tournament;"'
alias player_scores='psql-db -c "select * from player_score;"'
alias games='psql-db -c "select * from game;"'

alias unit-tests='python -m unittest discover -s unit_tests --pattern=*.py'
